{
    "restore_on_startup": "empty_tab",
    "git_panel": {
        "tree_view": false,
    },
    "icon_theme": "Material Icon Theme",
    "theme": "Catppuccin Mocha - No Italics",
    "title_bar": {
        "show_onboarding_banner": false,
        "show_project_items": false,
        "show_branch_name": false,
        "show_user_menu": false,
        "show_sign_in": false,
    },
    "tab_bar": {
        "show": false,
    },
    "toolbar": {
        "quick_actions": false,
        "breadcrumbs": false,
    },
    "project_panel": {
        "dock": "right",
        "default_width": 400,
        "hide_root": true,
        "auto_fold_dirs": false,
        "starts_open": false,
        "git_status": false,
        "sticky_scroll": false,
        "scrollbar": {
            "show": "never",
        },
        "indent_guides": {
            "show": "never",
        },
    },
    "outline_panel": {
        "default_width": 300,
        "indent_guides": {
            "show": "never",
        },
    },
    "file_finder": {
        "modal_max_width": "large",
    },
    "scrollbar": {
        "show": "never",
    },
    "gutter": {
        "min_line_number_digits": 0,
        "folds": false,
        "runnables": false,
    },
    "ui_font_family": "JetBrainsMono Nerd Font Mono",
    "buffer_font_family": "JetBrainsMono Nerd Font Mono",
    "tab_size": 4,
    "format_on_save": "on",
    "autosave": {
        "after_delay": {
            "milliseconds": 1,
        },
    },
    "auto_update": false,
    "diagnostics": {
        "inline": {
            "enabled": true,
        },
    },
    "soft_wrap": "editor_width",
    "inlay_hints": {
        "enabled": true,
    },
}
